package io.redeasy.shopcar.entity;

import jakarta.persistence.Column;
import jakarta.persistence.Entity;

@Entity
public class Car {

	public Long id;
	
	@Column(length = 16)
	String model;
	
	@Column(length = 16)
	String bodyType;
	
	@Column(length = 16)
	String brand;
	
	String year;
	
	Double price;
	
	int seats;
	
	int doors;
	
	@Column(length = 128)
	String urlImage;
}
